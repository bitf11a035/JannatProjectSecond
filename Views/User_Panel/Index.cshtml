                    <li><a href="/Admin_Panel/Index_Admin">Home</a></li>
                    <li><a class="smoothscroll" href="#comp">Complaints</a></li>
                    <li><a class="smoothscroll" href="#search">Search</a></li>
                    <li>@Html.ActionLink("Logout", "IndexAdmin", "Admin_Panel")</li>                 

