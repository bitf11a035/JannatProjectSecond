    <section id='login'>
       
      @using (Html.BeginForm("Update2", "Admin_Panel",  new { enctype = "multipart/form-data" }))
       {

            <div>

                <div style="padding-left:100px">
                    <div>
                        <h5>Profile Picture</h5>
                        <div style="width:215px;height:165px;border:8px inset black;">
                            <img src="~/Data/@ViewBag.X.photo" alt="qwert" height="150" width="200" />
                            <input type="File" id="image" name="image" value=@ViewBag.X.photo />

                        </div>

                    </div>

                </div>
                <div style="padding-top:0px">

                    <div id="navigation" style="padding-top:0px">

                        @*<form action="~/User_Panel/Update2">*@
                        <table>
                            <tr><td><input type="hidden" id="id" name="id" value=@ViewBag.X.admin_id /></td></tr>
                            <tr>
                                <td><h5>Your Name is:&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</h5></td>
                                <td><input type="text" id="name" name="name" value=@ViewBag.X.name /></td>

                            </tr>
                            <tr>
                                <td><h5>Your email is:&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</h5></td>
                                <td><input type="text" id="email" name="email" value=@ViewBag.X.email /></td>

                            </tr>
                            <tr>
                                <td><h5>Your Phone No is:&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</h5></td>
                                <td><input type="text" id="phone_" name="phone_" value=@ViewBag.X.phone /></td>

                            </tr>
                            <tr>
                                <td><h5>Your Secondary Email is:&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</h5></td>
                                <td><input type="text" id="second" name="second" value=@ViewBag.X.secondary_email /></td>

                            </tr>
                            <tr>
                                <td><h5>Your Password is:&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</h5></td>
                                <td><input type="text" id="pass" name="pass" value=@ViewBag.X.passowrd /></td>

                            </tr>
                            <tr>
                                <td><h5>Your City is:&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</h5></td>
                                <td><input type="text" id="city" name="city" value=@ViewBag.X.city  /></td>

                            </tr>
                            <tr>
                                <td><h5>Your Street is:&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</h5></td>
                                <td><input type="text" id="street" name="street" value=@ViewBag.X.street /></td>

                            </tr>
                            <tr>
                                <td><h5>Your House No is:&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</h5></td>
                                <td><input type="text" id="house" name="house" value=@ViewBag.X.houseNo /></td>

                            </tr>
                        </table>
                        <input type="submit" value="Update" />

                        @* </form>*@
                    </div>
                </div>
            </div>
      }
           
        <div style="padding-top:500px"></div>


